const {resolve} = require('path');

let env
if (process.argv.includes('dev')) env = 'dev'

const basePath = env==='dev'
    ? resolve(__dirname, './src')
    : resolve(__dirname, './')
    
module.exports = {
    config: '.sequelize.js',
    'seeders-path': resolve(basePath, 'database/seeders'),
    'migrations-path': resolve(basePath, 'database/migrations'),
};
